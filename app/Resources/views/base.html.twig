<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}AManager{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        <div class="container-fluid">
            <div class="row">
                <div class="container col-md-2">
                    <div class="row">
                        <div id="logo" class="">
                            <a href="{{ path('homepage') }}">
                                <img src="/image/logo.png" width="256" height="256" alt="logo">
                            </a>
                        </div>
                    </div>
                    <div class="row">
                        <div id="menu" class="">{{ knp_menu_render('AppBundle:Builder:mainMenu') }}</div>
                    </div>
                </div>
                <div class="col-md-8">{% block body %}{% endblock %}</div>
                {% block javascripts %}{% endblock %}
            </div>
        </div>
    </body>
</html>
